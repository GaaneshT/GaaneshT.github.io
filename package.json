		"postcss": "^8.4.49",
		"svelte": "^5.0.0",
		"svelte-check": "^4.0.0",
		"tailwindcss": "^3.4.17",
		"typescript": "^5.0.0",
		"vite": "^5.4.11"
	},
	"dependencies": {
		"@fortawesome/fontawesome-free": "^6.7.2",
		"@fortawesome/fontawesome-svg-core": "^6.7.2",
